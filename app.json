{
  "expo": {
    "name": "chatbot-app-expo",
    "slug": "chatbot-app-expo",
    "version": "1.0.1",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "sdkVersion": "48.0.0",
    "plugins": [
      [
        "expo-updates",
        {
          "username": "antoine-anthony-wt"
        }
      ],
      [
        "expo-av"
      ]
    ],
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.antoineanthonywt.chatbotappexpo",
      "buildNumber": "7"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff",
        "versionCode": 7
      },
      "permissions": [
        "android.permission.RECORD_AUDIO",
        "android.permission.MODIFY_AUDIO_SETTINGS"
      ],
      "package": "com.antoineanthonywt.chatbotappexpo"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "runtimeVersion": {
      "policy": "sdkVersion"
    },
    "extra": {
      "eas": {
        "projectId": "e7564112-6142-415d-ac10-251111e31ae9"
      }
    },
    "updates": {
      "url": "https://u.expo.dev/e7564112-6142-415d-ac10-251111e31ae9"
    }
  }
}
