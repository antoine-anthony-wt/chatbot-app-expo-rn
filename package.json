{
  "name": "chatbot-app",
  "version": "1.0.1",
  "main": "node_modules/expo/AppEntry.js",
  "scripts": {
    "start": "APP_ENV=development npx expo start",
    "start:staging": "APP_ENV=staging npx expo start",
    "start:production": "APP_ENV=production npx expo start --no-dev --minify",
    "android": "npx expo start --android",
    "ios": "npx expo start --ios",
    "web": "npx expo start --web",
    "nuke:modules": "rm -rf node_modules && rm -rf yarn.lock && yarn cache clean && yarn install",
    "eas:submit:ios": "eas build --profile production -p ios --non-interactive --auto-submit",
    "eas:submit:android": "eas build --profile production -p android --non-interactive --auto-submit",
    "eas:update:ios": "eas update -p ios --branch production",
    "eas:update:android": "eas update -p android --branch production"
  },
  "dependencies": {
    "@expo/webpack-config": "^18.0.1",
    "@react-native-async-storage/async-storage": "1.17.11",
    "@react-navigation/native": "^6.1.6",
    "@react-navigation/stack": "^6.3.16",
    "axios": "^1.3.4",
    "buffer": "^6.0.3",
    "expo": "~48.0.9",
    "expo-av": "~13.2.1",
    "expo-constants": "~14.2.1",
    "expo-file-system": "~15.2.2",
    "expo-status-bar": "~1.4.4",
    "expo-updates": "~0.16.3",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-native": "0.71.6",
    "react-native-gesture-handler": "~2.9.0",
    "react-native-gifted-chat": "^2.0.0",
    "react-native-reanimated": "~2.14.4",
    "react-native-safe-area-context": "4.5.0",
    "react-native-screens": "~3.20.0",
    "react-native-web": "~0.18.10",
    "react-query": "^3.39.3"
  },
  "devDependencies": {
    "@babel/core": "^7.20.0",
    "@types/react": "~18.0.14",
    "arkit": "^1.6.4",
    "react-native-dotenv": "^3.4.8",
    "react-native-svg-transformer": "^1.0.0",
    "typescript": "^4.9.4"
  },
  "private": true
}
